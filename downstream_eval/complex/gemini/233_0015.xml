    <TextRegion id="region_0" custom="type:main_text">
      <Coords points="473,383 2368,383 2368,757 473,757"/>
      <TextLine id="line_0">
        <Coords points="473,383 2368,383 2368,422 473,422"/>
        <TextEquiv>
          <Unicode>वैश्वदेव कर्म स्वधा होम स्वाध्यायातिथि स क्रियाः ॥भूत पितृमरुद् ब्रह्म मनुष्याणामहामखाः॥ देवे</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_1">
        <Coords points="473,431 2210,431 2210,470 473,470"/>
        <TextEquiv>
          <Unicode>ष्वप्रहुतान्ना देवाञ्छूतवलि हरेत् ॥अन्नमन्नमथ चांडाल वायसेभ्यं च क्षिपेत् ॥अन्नं पितृमनुष्ये</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_2">
        <Coords points="473,479 2210,479 2210,518 473,518"/>
        <TextEquiv>
          <Unicode>भ्यो देयमप्यन्नं ह्यजला ॥स्वाध्यायं चा न्हिकं कुर्यान्न पैचेन्न चात्मने ॥बालखर्वासिनी वृद्ध दाग्निरारा</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_3">
        <Coords points="473,527 2210,527 2210,566 473,566"/>
        <TextEquiv>
          <Unicode>तुरकन्याकाः॥ संजोषातिथिज्जात्याश्रद्धपलोष भोजनं ॥प्राणाग्निहोत्र विधिनां जुजीतान्नम</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_4">
        <Coords points="473,575 2210,575 2210,614 473,614"/>
        <TextEquiv>
          <Unicode>धापहा ॥श्रितं विश्वं कं च हितं जंततस्त्री पूर्वकं ॥प्राणोप्राणो नेनोपरि एतदध्याद मृतातप</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_5">
        <Coords points="473,623 2210,623 2210,662 473,662"/>
        <TextEquiv>
          <Unicode>अन्नमन्नमृत्तं चैव कार्यमन्नैर्द्विजन्मना ॥अतिथि लेनवर्षां देयश्चात्यानुपूर्वशः॥अन्नं</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="473,671 2210,671 2210,709 473,709"/>
        <TextEquiv>
          <Unicode>प्रक्षोद्योतिथिः साय मपिवाग्मूत रोदकैः ॥सत्कृत्य निजवेदिज्ञात तत्वा स्रवता पचो</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="473,719 2210,719 2210,757 473,757"/>
        <TextEquiv>
          <Unicode>काले सखि संवधिवांधवा ॥महोत्सवं महाजनेभ्यो यावत्कल्पयेत् ॥सत्कृ</Unicode>
        </TextEquiv>
      </TextLine>
    </TextRegion>
