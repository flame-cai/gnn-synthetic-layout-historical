    <TextRegion id="region_5" custom="type:main_text">
      <Coords points="350,135 1987,135 1987,434 350,434"/>
      <TextLine id="line_5">
        <Coords points="375,140 1975,140 1975,168 375,168"/>
        <TextEquiv>
          <Unicode>समिद्धेवाप्यन्यासं संस्वादु भोजनं सूनृतं वचः ॥ प्रतिसंवत्सरं श्राद्धीः स्नातकाचार्यपाथिविः</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="375,172 1975,172 1975,205 375,205"/>
        <TextEquiv>
          <Unicode>वरः येदिवाल्यमृतं पापं संसृजेत् पुनः ॥ जघनीनोतिथिर्योयः श्रोत्रियोवेदपारगः ॥ मान्यावैतौपर</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="375,210 1975,210 1975,243 375,243"/>
        <TextEquiv>
          <Unicode>ह्यस्यस्वर्णलोकमभीप्सतः ॥ परपाकरुचिर्नस्यादनेघामंत्रणा हते ॥ वाक्यादिपांखापलं व</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_8">
        <Coords points="375,247 1975,247 1975,280 375,280"/>
        <TextEquiv>
          <Unicode>र्ययेजातिभोजनं ॥ प्रतिथिं श्रोत्रियं यत्प्रमासीमतादनुव्रजेत् ॥ जं हं शेषंस हासीत दिरे रे</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_9">
        <Coords points="375,285 1975,285 1975,317 375,317"/>
        <TextEquiv>
          <Unicode>श्रवंधुभिः ॥ उपास्पयथिसं संध्यां दुस्ताग्नी सानुपास्यच ॥ त्सैपरिजतोन्नु कानातिटप्रासं</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_10">
        <Coords points="375,322 1975,322 1975,355 375,355"/>
        <TextEquiv>
          <Unicode>विशेषात् ब्राह्मणेभ्यो हृतैषोस्यापयंचित्पेदलमनो हितं ॥ धर्मीपंकामान्स्वेकालेपयथाशक्तिः</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_11">
        <Coords points="375,359 1975,359 1975,392 375,392"/>
        <TextEquiv>
          <Unicode>र्महापयेत् ॥ विधाकर्मवयोबंधुवित्ते श्रीन्यापयथाक्रमं ॥ एतैः प्रवृत्तैः षोडोपिवादे को मानमर्ह</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_12">
        <Coords points="375,397 1975,397 1975,430 375,430"/>
        <TextEquiv>
          <Unicode>ति ॥ वदनांरिनपस्नातस्त्रीरोगवरचक्रिणां ॥ पादेषोन्नपसेषां मान्यास्नातप्रनूपतेः ॥ इड्या</Unicode>
        </TextEquiv>
      </TextLine>
    </TextRegion>
