    <TextRegion id="region_0" custom="type:main_text">
      <Coords points="92,59 2425,59 2425,586 92,586"/>
      <TextLine id="line_0">
        <Coords points="92,59 2425,59 2425,87 92,87"/>
        <TextEquiv>
          <Unicode>किनीचैर्वयोजायांदेदेतानि काउनिः जीवंलांछयमा यानुसुरूपनसंमहितद ॥</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_1">
        <Coords points="92,87 2425,87 2425,121 92,121"/>
        <TextEquiv>
          <Unicode>ब्राह्मणेनक्षत्रियायांउत्पादितः क्षत्रियः एकक्षत्रियेवैश्यायांउत्पादितः वैश्यः एकवैश्ये शूद्रायांउत्पादितः शूद्रः एकशूद्रः</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_2">
        <Coords points="700,255 2425,255 2425,298 700,298"/>
        <TextEquiv>
          <Unicode>ज्योतिः पुत्राश्रवा ईके ॥ योनौज्ञातयसेपंचांत अनेकवित् स्त्रियः पितृमातृसुत भ्रातृस्व</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_3">
        <Coords points="700,298 2425,298 2425,339 700,339"/>
        <TextEquiv>
          <Unicode>श्वशुरमातुलैः ॥ हीनान्नस्यादिनाजागृहीणान्यथाचवेता ॥ पतिप्रियहितेषु कास्वाचा</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_4">
        <Coords points="700,339 2425,339 2425,380 700,380"/>
        <TextEquiv>
          <Unicode>राविजेतेद्रया ॥ सेहकीर्त्तिमकाप्रोतिप्रेत्यचानुत्तमांगतिं ॥ सत्यामन्यां सवर्णीयां धर्मकार्यं</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_5">
        <Coords points="700,380 2425,380 2425,422 700,422"/>
        <TextEquiv>
          <Unicode>नकारयेत् ॥ सवरत्नि सुविधौ धर्मेज्येष्ठयानविनेतराः ॥ दाहयित्वाजिन होत्रैस्त्रियं वतिवत्तीपतिः २५</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="700,422 2425,422 2425,463 700,463"/>
        <TextEquiv>
          <Unicode>श्राद्धरे विधिवद्दारानग्नीप्रेवाविलंवयन ॥ इतिविवाहप्रकरणं ॥ सवरत्निः सवर्णासुजायते</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="700,463 2425,463 2425,503 700,503"/>
        <TextEquiv>
          <Unicode>हिसजातिपः ॥ जुगुप्तेषुविवाहेषुपुत्रासंतानवर्द्धनाः ॥ विप्रारमूसीवसेतोहि क्षत्रियायांविशति</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_8">
        <Coords points="700,503 2425,503 2425,545 700,545"/>
        <TextEquiv>
          <Unicode>पो ॥ जुगुप्तेः मूद्रांनिवादेजात पारशवोपिवा ॥ वैश्या मूद्रोस्तुरजन्यान्नाहिष्योन्नौस्तत्तौ</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_9">
        <Coords points="700,545 2425,545 2425,586 700,586"/>
        <TextEquiv>
          <Unicode>वैश्यातुकररासूद्रांविन्नास्वेवचविः सत्तूः ॥ ब्राह्मण्यां क्षत्रियात्सूतो वैश्यादेदेहि कस्तथा</Unicode>
        </TextEquiv>
      </TextLine>
    </TextRegion>
