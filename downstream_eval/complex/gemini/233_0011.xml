    <TextRegion id="region_2" custom="type:main_text">
      <Coords points="280,309 2475,309 2475,928 280,928"/>
      <TextLine id="line_0">
        <Coords points="280,309 2475,309 2475,386 280,386"/>
        <TextEquiv>
          <Unicode>आगर्नसंज्ञवा रुदे तपति तस्नपच्याजवेते॥ छनेज वधिनाजात क्षेत्रोर्प्यम वेत्सुत॥१॥ हताधि</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_1">
        <Coords points="280,386 2475,386 2475,464 280,464"/>
        <TextEquiv>
          <Unicode>नांपिंडमाचोपजीविनी॥ परेनूतामद्यः श्वांपावासयेद्यौचैचारिणी॥२॥ सोमः शौचंद दोछी सोंगु</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_2">
        <Coords points="280,464 2475,464 2475,541 280,541"/>
        <TextEquiv>
          <Unicode>नांगिरं॥ पावकः सर्वमेधपंतमेधावैयोचितोहतः॥३॥ व्यभिचारा हतौपुत्रौर्जेल्यागोविधीयते॥ गर्भा</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_3">
        <Coords points="280,541 2295,541 2295,618 280,618"/>
        <TextEquiv>
          <Unicode>नैवधादैचतथा महतिपातके॥ सुरापीव्याधिनायत्तीवर्थ्यर्धप्रिवदा॥४॥ स्त्रीप्रसूयाधिवैतवा</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_4">
        <Coords points="280,618 2475,618 2475,696 280,696"/>
        <TextEquiv>
          <Unicode>पुरुषदेविराणीतथा॥ उग्रधिविन्नापिकर्तव्यामहदेनोप्यप्यजवेते॥ यत्रानुकूलंद सोखिवर्गी</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_5">
        <Coords points="280,696 2475,696 2475,773 280,773"/>
        <TextEquiv>
          <Unicode>सच्चवर्द्धते॥ एतेजी वतिवापत्तौयाना न्यन्यमुपगच्छति॥ से हकीर्तिमवाप्रोतिमोदतेचोभयास</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="280,773 2475,773 2475,850 280,850"/>
        <TextEquiv>
          <Unicode>ह॥ प्राज्ञासंपादिनीदंक्षावीरसूंधिववादिनी॥ सज्जूरदाप्यस्तृतीयांशमदोनरएं स्त्रियां॥</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="280,850 2475,850 2475,928 280,928"/>
        <TextEquiv>
          <Unicode>निर्जर्तवचः कार्य्यमेषः धर्मः परस्त्रियाः॥ प्राप्यं देः संप्रतीस्योहि महा पातक दूषितः॥ लोकानं</Unicode>
        </TextEquiv>
      </TextLine>
    </TextRegion>
