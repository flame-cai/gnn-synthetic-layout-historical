        <Coords points="45,180 2462,180 2462,237 45,237"/>
        <TextEquiv>
          <Unicode>पा०वः ष सांचनारोशांश्शीर्धका॥इतिहांसांस्तथाविघां॥शक्त्राधीतेतेयेचहां॥मांसक्षीरोदनमधुतर्पसांस</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="45,237 2462,237 2462,294 45,294"/>
        <TextEquiv>
          <Unicode>दिवौकसां॥करोतिपितृकुर्यात्पितृ॥सामधुसपिषा॥तैत्प्राप्तर्ययेत्सर्वकामफलैः॥पुत्रैः॥यैच तेदेवाः पितरः</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="45,294 2462,294 2462,351 45,351"/>
        <TextEquiv>
          <Unicode>क्रतुमधयीततस्याप्नुयात्फलं॥निवृत्तपूर्वापृथिवीदानस्यफलमश्नुते॥तपसश्चपरस्नेह</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_8">
        <Coords points="45,351 2462,351 2462,408 45,408"/>
        <TextEquiv>
          <Unicode>निलंस्त्वाध्यावत् द्विजः॥नैष्ठिकोब्रह्मचारीतु व सेदाचार्यसंनिधौ॥तदनावेस्वपनयेपन्नवांवा</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_9">
        <Coords points="45,408 2462,408 2462,465 45,465"/>
        <TextEquiv>
          <Unicode>नरेविघा॥अजुनेनविधेनादेवान्साधयन्विजितेंद्रियः॥ब्रह्मलोकमाप्नोतिनचैहाजायतेपुनः॥</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_10">
        <Coords points="45,465 2462,465 2462,521 45,521"/>
        <TextEquiv>
          <Unicode>५॥इतिब्रह्मचारिप्रकरशा॥गुरुवेनुवरदत्वास्नायीतदनुज्ञापा॥वे स्वतानिवाकरनीत्वाअयुन</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_11">
        <Coords points="45,521 2462,521 2462,578 45,578"/>
        <TextEquiv>
          <Unicode>यमेववा॥अजुवृतब्रह्मचर्येणसंस्क्रियंममदहे॥ताअजनयपूर्वेकांकातामसिपंडांनजीयसीं॥ राम४</Unicode>
        </TextEquiv>
      </TextLine>
