    <TextRegion id="region_2" custom="type:main_text">
      <Coords points="250,218 2250,218 2250,484 250,484"/>
      <TextLine id="line_2">
        <Coords points="250,218 2250,218 2250,256 250,256"/>
        <TextEquiv>
          <Unicode>यतावावृतक्षत्रविशां कालजौपनायनिकः परः॥छूतः अर्द्धपतेते सर्वधर्मवहिष्कृता॥सावित्री</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_3">
        <Coords points="250,256 2250,256 2250,294 250,294"/>
        <TextEquiv>
          <Unicode>पतितावात्यावामलोपाहते कृतो मातुर्य्यद्ग्रे जायते तीय मौनिबंधनात्॥ब्राह्मण क्षत्रिय वैश्या छद्मद्विजकारः</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_4">
        <Coords points="250,294 2250,294 2250,332 250,332"/>
        <TextEquiv>
          <Unicode>स्मादेते द्विजास्ताः॥प्रज्ञानातपसाचैववृद्धानां चैव कर्मणां॥वेदैरेव द्विजानीनां निःश्रे</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_5">
        <Coords points="250,332 2250,332 2250,370 250,370"/>
        <TextEquiv>
          <Unicode>यसकरः परः॥मधुनापयसाचैवदेवांस्तर्पयदेजः॥पितृनृधिच धान्याचचोधीतेतुयोव</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_6">
        <Coords points="250,370 2250,370 2250,408 250,408"/>
        <TextEquiv>
          <Unicode>न्द्रः॥हांय्यैषिका कीर्त्तीतेयोन्हं सद्यताम्रतैः॥प्रीणातिदेवानां स्वेन मधुनापिट्स्था॥स</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_7">
        <Coords points="250,408 2250,408 2250,446 250,446"/>
        <TextEquiv>
          <Unicode>तुसेमघटैदेवांस्तर्पयेद्योन्हं पठेत॥सामानिते पितृपीडिते रामधुसत्पयेदसात्प</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="line_8">
        <Coords points="250,446 2250,446 2250,484 250,484"/>
        <TextEquiv>
          <Unicode>येदेवान्पर्पि रसः पठन्॥पितृन्मधुसर्पिष्यान्नं शक्तितोपि द्विजः॥नाकोवांछपुरा</Unicode>
        </TextEquiv>
      </TextLine>
    </TextRegion>
